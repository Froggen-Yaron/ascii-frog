{
  "name": "ascii-frog",
  "version": "3.0.36",
  "description": "ASCII Frog Generator - A fun web service to generate colorful ASCII art frogs",
  "type": "module",
  "scripts": {
    "predev": "npm install && cd frontend && npm install && cd ../backend && npm install && cd ..",
    "dev": "concurrently \"cd backend && npm run dev\" \"cd frontend && npm run dev\"",
    "build": "cd frontend && npm run build",
    "test": "cd backend && npm run test",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build",
    "version:patch": "npm version patch",
    "version:frontend": "cd frontend && npm version patch",
    "version:backend": "cd backend && npm version patch"
  },
  "keywords": [
    "ascii-art",
    "frog",
    "monorepo",
    "workspace"
  ],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github.com:FrogGen/ascii-frog.git"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "npm-run-all": "^4.1.5"
  }
}