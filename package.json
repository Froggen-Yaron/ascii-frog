{
  "name": "ascii-frog",
  "version": "3.0.33",
  "description": "ASCII Frog Generator - A fun web service to generate colorful ASCII art frogs",
  "main": "backend/server.js",
  "bin": {
    "ascii-frog": "backend/server.js"
  },
  "scripts": {
    "start": "node backend/server.js",
    "predev": "npm install",
    "dev": "concurrently \"PORT=3001 nodemon backend/server.js\" \"npm run frontend:dev\"",
    "test": "node backend/test/health-check.js",
    "frontend:dev": "cd frontend && vite --port 3000",
    "frontend:build": "cd frontend && vite build",
    "frontend:preview": "cd frontend && vite preview",
    "build": "npm run frontend:build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github.com:FrogGen/ascii-frog.git"
  },
  "type": "module",
  "dependencies": {
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "helmet": "^8.1.0"
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "browser-sync": "^3.0.4",
    "concurrently": "^9.2.0",
    "nodemon": "^3.1.10",
    "typescript": "^5.9.2",
    "vite": "^7.1.2"
  }
}
