{
  "name": "ascii-frog",
  "version": "1.0.0",
  "description": "ASCII Frog Generator - A fun web service to generate colorful ASCII art frogs",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "npm run frontend:dev",
    "dev:full": "concurrently \"PORT=3001 nodemon backend/server.js\" \"npm run frontend:dev\"",
    "test": "node backend/test/health-check.js",
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "frontend:preview": "cd frontend && npm run preview",
    "build": "npm run frontend:build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github-playground.jfrogdev.org:FrogGen/ascii-frog.git"
  },
  "type": "commonjs",
  "dependencies": {
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "helmet": "^8.1.0"
  },
  "devDependencies": {
    "browser-sync": "^3.0.4",
    "concurrently": "^9.2.0",
    "nodemon": "^3.1.10",
    "vite": "^5.4.0"
  }
}