{
  "name": "ascii-frog",
  "version": "3.0.35",
  "description": "ASCII Frog Generator - A fun web service to generate colorful ASCII art frogs",
  "type": "module",
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "predev": "npm install",
    "dev": "concurrently \"npm run -w backend dev\" \"npm run -w frontend dev\"",
    "build": "npm run -w frontend build",
    "test": "npm run -w backend test",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build"
  },
  "keywords": [
    "ascii-art",
    "frog",
    "monorepo",
    "workspace"
  ],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github.com:FrogGen/ascii-frog.git"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "npm-run-all": "^4.1.5"
  }
}
