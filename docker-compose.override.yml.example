# Example: How to build with npm authentication
# Copy this to docker-compose.override.yml and add your token

version: '3.8'

services:
  ascii-frog:
    build:
      context: .
      dockerfile: Dockerfile
      args:
        # Get this from your ~/.npmrc file
        NPM_AUTH_TOKEN: "your-jfrog-auth-token-here"
    ports:
      - "3000:3000"
